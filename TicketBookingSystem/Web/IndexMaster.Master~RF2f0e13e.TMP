<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="IndexMaster.master.cs" Inherits="TicketBookingSystem.Web.IndexMaster" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>My Tickets</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <!-- Bootstrap CSS -->
    <link href="../ReferenceFile/css/style.css" type="text/css" rel="stylesheet" />
    <link href="../ReferenceFile/css/bootstrap-datepicker.css" type="text/css" rel="stylesheet" />
    <!-- Favicon -->
    <!-- Favicon and Touch Icons -->
    <link href="https://cdn.jsdelivr.net/npm/@fortawesome/fontawesome-free@5.14/svgs/solid/search-location.svg" rel="shortcut icon" type="image/png" />
    <link href="/ReferenceFile/images/apple-touch-icon.png" rel="apple-touch-icon" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.12.1/css/all.min.css" type="text/css" rel="stylesheet" />
    <link href="../ReferenceFile/OwnStyle.css" rel="stylesheet" />
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
    <style>
        a.btn-action {
            background: #222;
            color: #fff;
            padding: 9px 13px;
            margin: 0 0 0 15px;
        }
    </style>
</head>
<body>
    <form runat="server">
        <!-- =======================
	header Start-->
        <header class="header-static navbar-sticky navbar-light shadow">
            <!-- Logo Nav Start -->
            <nav class="navbar navbar-expand-lg">
                <div class="container">
                    <!-- Logo -->
                    <a class="navbar-brand" href="index.html" style="color: white; font-family: comic sans ms;">
                        <i class="fas fa-search-location"></i>&nbsp;MyTickets
                    </a>
                    <!-- Menu opener button -->
                    <button class="navbar-toggler ml-auto" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation"><span class="" style="color: white;"><i class="fas fa-bars"></i></span></button>
                    <!-- Main Menu Start -->
                    <div class="collapse navbar-collapse" id="navbarCollapse">
                        <ul class="navbar-nav ml-auto">
                            <!-- Menu item 1 Demos-->
                            <li class="nav-item dropdown"><a class="nav-link" href="#" id="demosMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fas fa-home"></i>&nbsp;Home</a>
                            </li>
                            <li class="nav-item dropdown"><a class="nav-link" href="#" id="docMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fas fa-bus-alt"></i>&nbsp;Bus</a>
                            </li>
                            <li class="nav-item dropdown"><a class="nav-link" href="#" id="docMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">⛴&nbsp;Launch</a>
                            </li>
                            <li class="nav-item dropdown"><a class="nav-link" href="#" id="docMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fas fa-plane-departure"></i>&nbsp;Air</a>
                            </li>
                            <li class="nav-item dropdown"><a class="nav-link" href="#" id="docMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fas fa-film"></i>&nbsp;Movie </a>
                            </li>
                            <li class="nav-item dropdown"><a class="nav-link" href="#" id="docMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fas fa-calendar-alt"></i>&nbsp;Events</a>
                            </li>

                            <!-- Menu item 5 Elements-->
                            <li class="nav-item dropdown megamenu"><a class="nav-link" href="/Web/SignUpOption.aspx" id="elementsMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <span>
                                    <asp:LinkButton class="btnSign" runat="server" ID="btnSign" OnClick="OnClick" Style="color: white;"><i class="fas fa-lock"></i>&nbsp;Sign up</asp:LinkButton>
                                </span>
                                <span id="userName" runat="server" visible="False" class="userName" style="font-size: 16px; color: lightseagreen;">
                                    <% HttpCookie cookies = HttpContext.Current.Request.Cookies["Ticket"];%>

                                    <%=cookies["Name"].ToString() %></span>
                            </a>
                            </li>
                            <!-- Menu item 6 Docs-->
                            &nbsp;
                            &nbsp;
                            <!-- Menu item 5 Elements-->
                            <li class="nav-item dropdown megamenu">
                                <a class="nav-link" href="/Web/Login.aspx" id="elementsMenu" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <% HttpCookie cookies = HttpContext.Current.Request.Cookies["Ticket"];%>
                                    <span>
                                        <asp:LinkButton class="btnSign" runat="server" ID="btnLogin" OnClick="btnLogin_OnClick" Style="color: white;"><i class="fas fa-lock-open"></i>&nbsp;Log in</asp:LinkButton>
                                    </span>
                                    <img src='<%=cookies["Name"].ToString() %>' id="userImage" runat="server" visible="False" style="height: 50px; width: 50px; border-radius: 50%; border: 0.8px solid #696969;" /></a>
                            </li>
                            <!-- Menu item 6 Docs-->

                        </ul>
                    </div>
                    <!-- Main Menu End -->
                    <!-- Header Extras Start-->
                    <div class="navbar-nav">
                    </div>
                    <!-- Header Extras End-->
                </div>
            </nav>
            <!-- Logo Nav End -->
        </header>
        <section>
            <div class="col-12 p-3">
                <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
                </asp:ContentPlaceHolder>
            </div>
        </section>
        <!-- =======================
	header End-->
        <!-- =======================
	Main Banner -->
        <!-- =======================
	Main banner -->
        <footer class="footer footer-dark position-relative">

            <!--footer copyright -->
            <div class="footer-copyright py-3">
                <div class="container">
                    <div class="d-md-flex justify-content-between align-items-center py-3 text-center text-md-left">
                        <!-- copyright text -->
                        <div class="copyright-text">©2020 All Rights Reserved by <a href="#!">Bits.</a></div>
                        <!-- copyright links-->

                    </div>
                </div>
            </div>
        </footer>
    </form>
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="/ReferenceFile/js/jquery.min.js" type="text/javascript"></script>
    <script src="/ReferenceFile/js/popper.min.js" type="text/javascript"></script>
    <script src="/ReferenceFile/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/ReferenceFile/js/functions.js" type="text/javascript"></script>
    <script src="/ReferenceFile/js/owl.carousel.min.js" type="text/javascript"></script>
    <script src="/ReferenceFile/js/slick.js" type="text/javascript"></script>
    <script src="/ReferenceFile/js/swiper.min.js" type="text/javascript"></script>
    <script src="/ReferenceFile/js/main.js" type="text/javascript"></script>
    <script src="/ReferenceFile/js/jquery.fancybox.min.js" type="text/javascript"></script>
    <script src="/ReferenceFile/js/bootstrap-datepicker.min.js" type="text/javascript"></script>
    <script src="/ReferenceFile/js/jquery-ui.min.js" type="text/javascript"></script>
</body>
</html>
